  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opencollector/kenall-js/issues"
  },
  "homepage": "https://github.com/opencollector/kenall-js#readme",
  "devDependencies": {
    "@babel/core": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@rollup/plugin-babel": "5.2.0",
    "@rollup/plugin-commonjs": "15.0.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.3",
    "@rollup/plugin-typescript": "5.0.2",
    "@types/gulp": "4.0.6",
    "@types/gulp-minify": "3.1.0",
    "@types/gulp-sourcemaps": "0.0.32",
    "@types/gulp-terser": "1.2.1",
    "@types/gulp-zip": "^4.0.1",
    "@types/jest": "26.0.12",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "core-js": "3.6.5",
    "eslint": "^7.20.0",
    "gulp": "4.0.2",
    "gulp-better-rollup": "4.0.1",
    "gulp-clean": "^0.4.0",
    "gulp-exec": "^5.0.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-terser": "1.4.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "gulp-zip": "^5.0.2",
    "jest": "26.4.2",
    "prettier": "^2.2.1",
    "rollup": "^2.26.9",
    "rollup-plugin-banner2": "^1.0.2",
    "superstruct-ts-transformer": "0.2.2",
    "ts-jest": "26.3.0",
