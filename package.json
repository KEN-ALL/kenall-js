{
  "name": "@ken-all/kenall",
  "version": "1.1.0",
  "description": "KENALL JavaScript SDK",
  "main": "built/index.js",
  "types": "built/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .ts src/",
    "format": "prettier --write src/",
    "check-format": "prettier -c src/",
    "build": "ttsc -d",
    "bundle": "gulp bundle",
    "release": "gulp release",
    "test": "jest",
    "doc:gen-json": "typedoc --generate-json translations/base",
    "doc:base": "typedoc --out built/docs",
    "doc:ja": "typedoc --out built/docs/ja --readme README.ja.md --generate-from-json translations/ja"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opencollector/kenall-js.git"
